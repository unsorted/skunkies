{
  "name": "@unsorted/skunkies",
  "private": true,
  "version": "1.0.0",
  "license": "CC0 1.0 Universal",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "export": "next export",
    "build:base-css": "tailwindcss build ./src/styles/tailwind.base.css -o ./src/styles/base.css",
    "start": "next start",
    "test": "yarn test:unit && yarn test:integ",
    "test:unit": "jest --config jest.config.js --projects tests/unit",
    "test:unit:coverage": "jest --config jest.config.js --projects tests/unit --coverage",
    "test:integ": "jest --config jest.config.js --projects tests/integ",
    "test:integ:coverage": "jest --config jest.config.js --projects tests/integ --coverage",
    "lint": "yarn lint:code",
    "lint:code": "eslint . --ext .ts,.tsx,.js,.jsx",
    "fix:staged": "lint-staged --allow-empty",
    "fix:all": "yarn fix:all:code",
    "fix:all:code": "eslint . --ext .ts,.tsx,.js,.jsx --fix --rule 'react-hooks/exhaustive-deps: off' --fix",
    "prettier": "prettier --write \"./src/**/*.{ts,tsx,scss,js,json}\"",
    "prettier:all": "prettier --write \"./src/**/*.{ts,tsx,scss,js,json}\"",
    "typecheck": "tsc -p . --noEmit",
    "deps:check": "npx npm-check-updates --dep prod,dev,optional",
    "deps:update": "npx npm-check-updates --dep prod,dev,optional -u"
  },
  "browserslist": {
    "production": [
      ">1%",
      "edge >= 14",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn fix:staged",
      "pre-push": "yarn run typecheck"
    }
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "^11.0.0-next.10",
    "@emotion/cache": "11.0.0-next.13",
    "@emotion/css": "11.0.0-next.14",
    "@emotion/react": "11.0.0-next.14",
    "@emotion/server": "11.0.0-next.14",
    "@emotion/styled": "11.0.0-next.14",
    "classnames": "^2.2.6",
    "date-fns": "^2.15.0",
    "framer-motion": "^2.3.0",
    "gray-matter": "^4.0.2",
    "isomorphic-unfetch": "^3.0.0",
    "ky": "^0.23.0",
    "ky-universal": "^0.8.1",
    "next": "^9.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-lazyload": "^3.0.0"
  },
  "devDependencies": {
    "@tailwindcss/ui": "^0.5.0",
    "@testing-library/jest-dom": "^5.11.2",
    "@testing-library/react": "^10.4.7",
    "@testing-library/react-hooks": "^3.4.1",
    "@types/classnames": "^2.2.10",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.8",
    "@types/node": "^14.0.27",
    "@types/react": "^16.9.44",
    "@types/react-dom": "^16.9.8",
    "@types/react-lazyload": "^2.6.0",
    "@typescript-eslint/eslint-plugin": "^3.7.1",
    "@typescript-eslint/parser": "^3.7.1",
    "babel-jest": "^26.2.2",
    "babel-plugin-macros": "^2.8.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jest-formatting": "^2.0.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.5",
    "eslint-plugin-react-hooks": "^4.0.8",
    "eslint-plugin-sonarjs": "^0.5.0",
    "husky": "^4.2.5",
    "jest": "^26.2.2",
    "jest-cli": "^26.2.2",
    "jest-enzyme": "^7.1.2",
    "jest-localstorage-mock": "^2.4.3",
    "lint-staged": "^10.2.11",
    "node-mocks-http": "^1.8.1",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "remark": "^12.0.1",
    "remark-codesandbox": "^0.5.2",
    "remark-html": "^12.0.0",
    "shell-quote": "^1.7.2",
    "tailwindcss": "^1.6.2",
    "ts-jest": "^26.1.4",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=10.17.0",
    "yarn": ">=1.17.0 <2.0.0"
  }
}
